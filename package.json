{
  "name": "trend-store",
  "private": true,
  "packageManager": "pnpm@9.12.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "pnpm -C apps/web dev",
    "worker": "pnpm -C apps/worker build && node apps/worker/dist/index.js",
    "migrate": "pnpm -C packages/db migrate",
    "seed": "pnpm -C packages/db seed"
  }
}